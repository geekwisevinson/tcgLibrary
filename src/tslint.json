{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "vf",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "vf",
            "kebab-case"
        ],
      "typedef-whitespace": [
        false,
        {
          "call-signature": "onespace",
          "index-signature": "onespace",
          "parameter": "onespace",
          "property-declaration": "onespace",
          "variable-declaration": "onespace"
        }
      ]
    }
}
